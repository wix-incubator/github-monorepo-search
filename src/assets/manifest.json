{
  "name": "Github monorepo search",
  "version": "0.1",
  "description": "Github monorepo search",
  "omnibox": {
    "keyword": "gh"
  },
  "manifest_version": 2,
  "icons": {
    "64": "cloud-computing.png"
  },
  "browser_action": {
    "default_icon": {
      "64": "cloud-computing.png"
    },
    "default_title": "",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      }
    }
  }
}