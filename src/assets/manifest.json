{
  "name": "",
  "version": "0.1",
  "description": "Better github search",
  "manifest_version": 2,
  "icons": {
    "64": "cloud-computing.png"
  },
  "browser_action": {
    "default_icon": {
      "64": "cloud-computing.png"
    },
    "default_title": "",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      }
    }
  }
}